				"resizable":true,
				"size":{"width":772, "height":620},
				"min_size":{"width":450, "height":300},
				"max_size":{"width":772, "height":620}
			},
		},
		"externally_connectable": {
			"matches": [
				"http://arche-base.com",
			]
		}
	},
	"launch_events": [
		{
			"event": "GameLaunch",
			"event_data": {
				"game_ids": [102881]
			},
			"start_minimized": true
		}
	]
