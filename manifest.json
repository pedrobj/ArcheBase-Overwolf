{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"ArcheBase",
		"version":"1.0.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"ArcheBase",
		"icon":"Icon.png",
		"icon_gray":"Icon_gray.png",
		"description":"ArcheAge Builder and Guides by ArcheBase"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
				"file":"Files/index.html",
