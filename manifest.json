{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"ArcheBase",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"ArcheBase",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"ArcheAge Builder and Guides by ArcheBase"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent":true,
					"resizable":true,
					"size":{"width":750, "height":500},
					"min_size":{"width":460, "height":297},
					"max_size":{"width":1200, "height":800}
				}
		}
	}
}