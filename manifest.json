{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"ArcheBase",
		"version":"1.0.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"ArcheBase",
		"icon":"Icon.png",
		"icon_gray":"Icon_gray.png",
		"description":"ArcheAge Builder and Guides by ArcheBase"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent":false,
					"resizable":false,
					"size":{"width":750, "height":500},
					"min_size":{"width":750, "height":500},
					"max_size":{"width":750, "height":500}
				}
		},
		"externally_connectable": {
			"matches": [
				"http://arche-base.com",
			]
		}
	},
	"launch_events": [
		{
			"event": "GameLaunch",
			"event_data": {
				"game_ids": [102881]
			},
			"start_minimized": true
		}
	]
}